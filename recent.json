ags":["은퇴","중년","철학","인문학","50대","아버지"],"filteredTags":null,"bizKeywords":["인문학","은퇴","50대","철학","중년"],"totalShareCount":null,"shareClickCount":null,"socialIncomingCount":0,"meta":{"_tromm":"economy","_app":"harmony"},"totalView":4,"recentView":0,"fbCommentCount":0,"useDefaultOpencomment":"true","viewCoverImage":"https://t1.daumcdn.net/liveboard/vintage/5c6216b142244f4ea8e349d469ad7569.jpg","pcCoverImage":"https://t1.daumcdn.net/liveboard/vintage/89499244f8aa4f4d8c881c65ec9fe8dd.jpg","useLatestOrderComment":"false","cp":null,"viewCoverVideo":null,"summary":[],"version":3,"title":"김부장이 회사를 그만뒀다","editor":"fbmXNjJCYX","updateTime":1597917612578,"regTime":1597901762897,"titleRaw":"김부장이 회사를 그만뒀다","img":"https://t1.daumcdn.net/liveboard/vintage/89499244f8aa4f4d8c881c65ec9fe8dd.jpg","path":"vintage/philosophy_for_u","isHiddenComment":"false","isDisabledComment":false,"isPublic":true},{"id":"5f3d3dda19527f32c56d65c7","channelKey":"animal","serviceKey":"bemypet","publishKey":"5f3d3dda19527f32c56d65c7","consumerId":"1boon","status":"published","description":"","coverImage":"https://t1.daumcdn.net/liveboard/bemypet/690fa059fa384d79bc3207ef3f94b6b5.png","serviceTemplate":"listicle","publishTime":1597917612392,"tags":["고양이","고양이툰","고양이만화","동물툰","동물만화","고양이집사","비마이펫툰","반려묘"],"filteredTags":null,"bizKeywords":["고양이","반려묘","고양이집사","고양이만화"],"totalShareCount":null,"shareClickCount":null,"socialIncomingCount":33,"meta":{"_tromm":"animal","_app":"harmony","_approvedAt":"1597917616825","_isApproved":"true"},"totalView":79,"recentView":0,"fbCommentCount":0,"useDefaultOpencomment":"true","viewCoverImage":null,"pcCoverImage":"https://t1.daumcdn.net/liveboard/bemypet/690fa059fa384d79bc3207ef3f94b6b5.png","useLatestOrderComment":"false","cp":null,"viewCoverVideo":null,"summary":[],"version":3,"title":"삼색이 고양이 이름 짓기","editor":"e2OzbU1JlL","updateTime":1597917612392,"regTime":1597849050085,"titleRaw":"삼색이 고양이 이름 짓기","img":"https://t1.daumcdn.net/liveboard/bemypet/690fa059fa384d79bc3207ef3f94b6b5.png","path":"bemypet/5f3d3dda19527f32c56d65c7","isHiddenComment":"false","isDisabledComment":false,"isPublic":true},{"id":"5f3c9212a223e9534e18fef0","channelKey":"library","serviceKey":"textat","publishKey":"1647","consumerId":"1boon","status":"published","description":"","coverImage":"https://t1.daumcdn.net/liveboard/textat/53d873d5004640759541d1e03a1a096c.jpeg","serviceTemplate":"listicle","publishTime":1597917612281,"tags":["비밀 연애","연애","연인","커플","연애의 과학","사내연애","CC","심리학"],"filteredTags":null,"bizKeywords":["연애","CC","심리학","커플","연인","사내연애","비밀연애","연애의과학"],"totalShareCount":null,"shareClickCount":null,"socialIncomingCount":0,"meta":{"_tromm":"relationship","_app":"harmony"},"totalView":3,"recentView":0,"fbCommentCount":0,"useDefaultOpencomment":"true","viewCoverImage":null,"pcCoverImage":"https://t1.daumcdn.net/liveboard/textat/53d873d5004640759541d1e03a1a096c.jpeg","useLatestOrderComment":"false","cp":null,"viewCoverVideo":null,"summary":[{"title":"","desc":"<br> \n<div style=\"margin:0 auto; border-top:2px solid #333333 ; width:15px\"></div> \n<br> \n<h4 style=\"text-align: center;\"><span style=\"font-size: 30px;\"><b>너랑 나랑은...</b></span></h4> \n<div> \n <b><br></b> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">아무도 모르는&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">너랑 나만의&nbsp;비밀 연애.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;약간 불편하긴 하지만&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">왠지 더 특별하고 설레는</span></strong> \n <span style=\"color: inherit;\"> </span> \n <strong><span style=\"color: #ff6600;\"> 느낌</span></strong> \n <span style=\"color: inherit;\">이 들지 않나요? </span> \n <img class=\"alignnone wp-image-7115\" src=\"http://scienceoflove.co.kr/wp-content/uploads/2016/12/sol202_illu_01-1.png\" width=\"373\" height=\"250\" style=\"color: inherit;\"> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"> 누가 우리 사이에 끼어들어서&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">이래라저래라 하지도 않고</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;사람들의 입에&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">오르내릴&nbsp;</span> \n <span style=\"color: inherit;\">걱정도 없기 때문에&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">일부러 비밀 연애를 하는 커플들도 있어요.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;그렇다면</span> \n <span style=\"color: rgb(51, 51, 51);\"> 비밀 연애의 단점은 </span> \n <span style=\"color: inherit;\">무엇일까요?&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;오늘은 </span> \n <strong><span style=\"color: #ff6600;\">연구를 통해 밝혀진</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">비밀 연애의 단점 세 가지</span></strong> \n <span style=\"color: inherit;\">를 알려드릴 거예요.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;만약 지금 비밀 연애를 하고 있거나,&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">할 예정이라면 이 글이 큰 도움이 될 겁니다! &nbsp;&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<br> \n<div style=\"margin:0 auto; border-top:2px solid #333333 ; width:15px\"></div> \n<br> \n<h4 style=\"text-align: center;\"><span style=\"font-size: 30px;\"><b>비밀 연애는 힘들어</b></span></h4> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">미국 공군사관학교의 크레이그 포스터 교수님은</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;564명을 대상으로 한 연구를 통해,&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">비밀 연애의 세 가지 단점을 정리했어요.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;&nbsp; </span> \n <b>#1. 비밀을 지켜야 한다는 압박감</b> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;비밀을 지키는 데는 생각보다&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">많은 정신력이 필요</span></strong> \n <span style=\"color: inherit;\">합니다.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">(Lane&amp;Wegner, 1995)&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\"><br></span></strong> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"></span> \n <strong><span style=\"color: #ff6600;\">‘들키면 안 돼!’라고</span></strong> \n <span style=\"color: inherit;\"> </span> \n <strong><span style=\"color: #ff6600;\">스스로&nbsp;</span></strong> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">계속 압박하고 있는 셈</span></strong> \n <span style=\"color: inherit;\">이거든요.</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"></span> \n <img class=\"alignnone wp-image-7118\" src=\"http://scienceoflove.co.kr/wp-content/uploads/2016/12/sol202_illu_02_.png\" width=\"326\" height=\"250\" style=\"color: inherit;\"> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;혹시 실수 할까 봐,&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">말이나 행동도 더 조심하게 되고&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">주변 사람의 눈치도 더 많이 보게 돼요.&nbsp; </span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">이러한 압박감은 생각보다 커서&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">실제로 정신적으로 더 빨리 지치고 말아요</span></strong> \n <span style=\"color: inherit;\">.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">당연히 관계에 좋을 리 없죠.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;&nbsp; </span> \n <b>#2. 낮은 관계 만족도</b> \n <span style=\"color: inherit;\">&nbsp; </span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">비밀 연애를 할 때는&nbsp;</span> \n <span style=\"color: inherit;\">제대로 된 데이트나&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">자유로운 애정표현을 하기 힘들어요.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">간단한 문자나 전화통화를 하려고 해도&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">혹시 티가 날까 봐 눈치를 보게 되고요.</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"></span> \n <img class=\"alignnone wp-image-7110\" src=\"http://scienceoflove.co.kr/wp-content/uploads/2016/12/sol202_illu_03.png\" width=\"314\" height=\"250\" style=\"color: inherit;\"> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;평범한 커플보다 서로 </span> \n <strong><span style=\"color: #ff6600;\">사랑을</span></strong> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"> </span> \n <strong><span style=\"color: #ff6600;\"> 속삭일 기회가 상대적으로 적다 보니,</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\"> 뜨거운 사랑을 유지하기가 더 힘듭니다</span></strong> \n <span style=\"color: inherit;\">.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;자</span> \n <span style=\"color: inherit;\">연히 그만큼 관계에 집중하기도 어렵고&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">관계 만족도도 낮아지기 쉬워요.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;&nbsp; </span> \n <b>#3. 불안한 관계</b> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;내 연인에게 사귀는 사람이 있다는 걸&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">주변에서 모른다면,&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">다른 이성이 접근하기 쉬워집니다</span></strong> \n <span style=\"color: inherit;\">.&nbsp; </span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">주변 사람들이 소개팅을 권유하고&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">심지어 직접적인 호감을 표시할 때도&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">지켜만 봐야하죠.</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"></span> \n <img class=\"alignnone wp-image-7111\" src=\"http://scienceoflove.co.kr/wp-content/uploads/2016/12/sol202_illu_04.png\" width=\"378\" height=\"250\" style=\"color: inherit;\"> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">연구에 따르면 </span> \n <strong><span style=\"color: #ff6600;\">주변 사람들이</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\"> 두 사람의 관계를&nbsp;지지해주는 게</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\"> 관계 안정성에 큰 도움</span></strong> \n <span style=\"color: inherit;\">이 된다고 해요.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">(Felmlee, 2001)&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;심지어 </span> \n <strong><span style=\"color: #ff6600;\">연애 사실을 주변에 알리면</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\"> 서로에게 더 잘해주려고 한다</span></strong> \n <span style=\"color: inherit;\">는&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">연구 결과도 있을 정도죠.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <b><a href=\"http://scienceoflove.co.kr/연애/더-행복한-연애를-위하여/sns-프로필에-연애중-표시-할까-말까/\" style=\"background-color: rgb(255, 255, 255);\">(참고: SNS에 연애 중인 것을 티 내야 하는 이유)</a></b> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"> </span> \n <img class=\"alignnone wp-image-7112\" src=\"http://scienceoflove.co.kr/wp-content/uploads/2016/12/sol202_illu_05.png\" width=\"322\" height=\"250\" style=\"color: inherit;\"> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;이러한 </span> \n <strong><span style=\"color: #ff6600;\">주변 사람들의 서포트가 없다면</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\"> 그만큼 관계는 불안정</span></strong> \n <span style=\"color: inherit;\">해질 수밖에 없어요. &nbsp;&nbsp;</span> \n <br> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<br> \n<div style=\"margin:0 auto; border-top:2px solid #333333 ; width:15px\"></div> \n<br> \n<h4 style=\"text-align: center;\"><span style=\"font-size: 30px;\"><b>이 사람이 내 연인이다!</b></span></h4> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">결론적으로 비밀 연애가 공개 연애보다&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">훨씬 힘들고 힘든 건 당연해요.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;그러니 </span> \n <strong><span style=\"color: #ff6600;\">피치 못할 사정이 아니라면</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\"> 비밀 연애는 안 하는 게 훨씬 좋습니다.</span></strong> \n <span style=\"color: inherit;\">&nbsp; </span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">하지만 정말 어쩔 수 없이&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">비밀 연애를 해야만 하는 상황이라면,&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">오늘 알려드린 </span> \n <strong><span style=\"color: #ff6600;\">비밀 연애의&nbsp;</span></strong> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">세 가지 단점을</span></strong> \n <span style=\"color: inherit;\"> </span> \n <strong><span style=\"color: #ff6600;\"> 꼭 기억해주세요.</span></strong> \n <span style=\"color: inherit;\">&nbsp; </span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">비밀 연애가 어떤 점 때문에&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">힘들어질 수 있는지 잘 이해한다면,&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">그런 부분을 더 조심할 수 있으니까요.&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <img class=\"alignnone wp-image-7113\" src=\"http://scienceoflove.co.kr/wp-content/uploads/2016/12/sol202_illu_06.png\" width=\"294\" height=\"250\" style=\"color: inherit;\"> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">마지막으로 연애의 과학이 드리는&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">행복한 연애를 이어가는 다양한 팁들을&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">한번 읽어보세요!&nbsp; </span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">정말 읽는 것만으로도&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">연애에 도움이 된다니까요?&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;--&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;참, 혹시 지금 마음에 둔 사람이 있나요?&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">그렇다면&nbsp;</span> \n <strong><span style=\"color: #ff6600;\">상대방과 주고받은 카톡을 분석해</span></strong> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"> </span> \n <strong><span style=\"color: #ff6600;\">서로에 대한 호감도를 알아보세요!</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><br></strong> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"></span> \n <strong>\"연애의 과학 앱\"</strong> \n <span style=\"color: inherit;\">에 있는</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"></span> \n <span style=\"color: rgb(51, 51, 51);\"><strong>\"카톡으로 보는 속마음\"</strong>은</span> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">인공지능 개발에 사용되는&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">최신 머신러닝 기술을 사용해</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong><span style=\"color: #ff6600;\">두 사람의 감정을 정확하게 분석한답니다.</span></strong> \n <span style=\"color: inherit;\">&nbsp;</span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\"><br></span> \n</div> \n<div style=\"text-align: center;\"> \n <span style=\"color: inherit;\">&nbsp;분석에 필요한 시간은 단 10초!</span> \n</div> \n<div style=\"text-align: center;\"> \n <strong> </strong> \n <strong>지금 바로 해보세요 : )</strong> \n</div>"},{"title":"지금 클릭해서 속마음을 알아보세요","desc":""}],"version":2,"title":"비밀 연애! 절대 하면 안 되는 4가지 이유","editor":"textat","updateTime":1597917612281,"regTime":1597805074767,"titleRaw":"비밀 연애! 절대 하면 안 되는 4가지 이유","img":"https://t1.daumcdn.net/liveboard/textat/53d873d5004640759541d1e03a1a096c.jpeg","path":"textat/1647","isHiddenComment":"false","isDisabledComment":false,"isPublic":true},{"id":"5f3c83fef2910a6deac7d0ee","channelKey":"trending","serviceKey":"onehomelife","publishKey":"5f3c83fef2910a6deac7d0ee","consumerId":"1boon","status":"published","description":"이것만 있으면 한 달치 음식은 먹을 수 있겠어요! 후후","coverImage":"https://t1.daumcdn.net/liveboard/onehomelife/cde92b532afe434ca6f6d0a63a095b79.png","serviceTemplate":"swipe","publishTime":1597917612136,"tags":["양념장","양념장백과사전","양념장요리","양념별요리","양념별레시피","고추장요리","간장요리","된장요리"],"filteredTags":null,"bizKeywords":["양념장","양념장백과사전","양념별레시피","된장요리","고추장요리","간장요리"],"totalShareCount":null,"shareClickCount":null,"socialIncomingCount":310,"meta":{"_tromm":"recipe","_app":"harmony","_approvedAt":"1597917614443","_isApproved":"true"},"totalView":329,"recentView":0,"fbCommentCount":0,"useDefaultOpencomment":"true","viewCoverImage":"https://t1.daumcdn.net/liveboard/onehomelife/04024627451546a5998e39bf1092d2de.png","pcCoverImage":"https://t1.daumcdn.net/liveboard/onehomelife/d8235da6fb004f23b1ee1c3cb79529fa.png","useLatestOrderComment":"false","cp":null,"viewCoverVideo":null,"summary":[],"version":2,"title":"양념장별로 정리한 호불호 없는 집밥요리 총모음!","editor":"h03xDqrqTH","updateTime":1597917612136,"regTime":1597801470641,"titleRaw":"양념장별로 정리한 호불호 없는 집밥요리 총모음!","img":"https://t1.daumcdn.net/liveboard/onehomelife/